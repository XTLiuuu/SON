html(lang="en")
  head
    title= title
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
    script(src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous")
    link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous")
    //link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous")
    //script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lato")
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css")
    block head
    
    body
      .w3-top
        .w3-bar.w3-white.w3-wide.w3-padding.w3-card 
          a.w3-bar-item.w3-button.w3-hover-teal(href='/')
            b SON
          .w3-right.w3-hide-small 
            a.w3-bar-item.w3-button.w3-hover-teal(href='/calendar/calendarD')
              i.fa.fa-calendar 
              |  Calendar  
            |
            a.w3-bar-item.w3-button.w3-hover-teal(href='/friend')
              i.fa.fa-group 
              |  Friends 
            |
            a.w3-bar-item.w3-button.w3-hover-teal(href='/notification')
              i.fa.fa-bullhorn 
              |  Notifications
            |
            a.w3-bar-item.w3-button.w3-hover-teal(href='/setting')
              i.fa.fa-cog 
              |  Settings    
            |
            if loggedIn
              a.w3-bar-item.w3-button.w3-hover-teal(href="/logout")
                i.fa.fa-close 
                |  Logout
              |
              if status=='0211'
                a.w3-bar-item.w3-button.w3-hide-small.w3-hover-teal(href="/users")
                  i.fa.fa-cog 
                  |  Google Users
                | 
            else
              a.w3-bar-item.w3-button.w3-teal(href="/auth/google")
                i.fa.fa-user 
                |  Login
      
    block content
    script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T" crossorigin="anonymous")
